      "cell_type": "code",
      "source": [
        "from datasets import load_dataset\n",
        "import os\n",
        "import torch\n",
        "from torch.utils.data import DataLoader\n",
        "from transformers import TrainingArguments # Import TrainingArguments\n",
        "\n",
        "os.makedirs(\"finetune_data\", exist_ok=True)\n",
        "\n",
        "# Load and process the datasets\n",
        "alpaca = load_dataset(\"tatsu-lab/alpaca\")['train']\n",
        "gsm8k = load_dataset(\"gsm8k\", \"main\")['train']\n",
        "codealpaca = load_dataset(\"sahil2801/CodeAlpaca-20k\")['train']\n",
        "\n",
        "# Format the datasets consistently\n",
        "alpaca = alpaca.map(lambda x: {\"instruction\": x[\"instruction\"], \"output\": x[\"output\"]})\n",
        "gsm8k = gsm8k.map(lambda x: {\"instruction\": x[\"question\"], \"output\": x[\"answer\"]})\n",
        "\n",
        "# Combine the datasets\n",
        "from datasets import concatenate_datasets\n",
        "combined_dataset = concatenate_datasets([alpaca, gsm8k, codealpaca])\n",
        "\n",
        "# Split into train and test sets\n",
        "train_dataset = combined_dataset.train_test_split(test_size=0.2, seed=42)['train']\n",
        "test_dataset = combined_dataset.train_test_split(test_size=0.2, seed=42)['test']\n",
        "\n",
        "def tokenize_function(examples):\n",
        "    # Ensure padding tokens are handled properly within the function\n",
        "    tokenizer.pad_token = tokenizer.eos_token\n",
        "    # Set the maximum sequence length\n",
        "    max_length = 1000  # or adjust as needed for your data and model\n",
        "    # Tokenize instruction and output together with truncation\n",
        "    # to ensure the combined length does not exceed max_length\n",
        "    encodings = tokenizer(\n",
        "        examples['instruction'],\n",
        "        examples['output'],\n",
        "        padding=\"max_length\",\n",
        "        truncation=True,\n",
        "        max_length=max_length,\n",
        "    )\n",
        "    # Create labels - same as input_ids shifted by one position\n",
        "    encodings['labels'] = encodings['input_ids'].copy()  # Create a copy for labels\n",
        "    # Return the tokenized examples\n",
        "    return encodings\n",
        "\n",
        "train_dataset = train_dataset.map(tokenize_function, batched=True)\n",
        "test_dataset = test_dataset.map(tokenize_function, batched=True)\n",
        "\n",
        "# Set the format for PyTorch - include 'labels'\n",
        "train_dataset.set_format(type='torch', columns=['input_ids', 'attention_mask', 'labels'])\n",
        "test_dataset.set_format(type='torch', columns=['input_ids', 'attention_mask', 'labels'])\n",
        "\n",
        "# Define training_args before using it in Trainer\n",
        "training_args = TrainingArguments(\n",
        "    output_dir=\"./results\",             # Output directory to save models\n",
        "    # Replace 'evaluation_strategy' with 'eval_strategy'\n",
        "    eval_strategy=\"epoch\",              # Evaluate after each epoch\n",
        "    learning_rate=5e-5,                 # Learning rate for optimization\n",
        "    per_device_train_batch_size=2,      # Batch size for training\n",
        "    per_device_eval_batch_size=2,       # Batch size for evaluation\n",
        "    num_train_epochs=3,                 # Number of epochs to train\n",
        "    weight_decay=0.01,                  # L2 regularization to prevent overfitting\n",
        "    logging_dir='./logs',               # Directory for storing logs\n",
        "    logging_steps=500,                  # How often to log training progress\n",
        "    # Change save_strategy to 'epoch' to align with eval_strategy\n",
        "    save_strategy=\"epoch\",           # Save the model every epoch\n",
        "    save_total_limit=2,                 # Limit the number of saved models\n",
        "    load_best_model_at_end=True         # Load the best model after training\n",
        ")\n",
        "\n",
        "# Update the data_collator to include 'labels'\n",
        "trainer = Trainer(\n",
        "    model=model,\n",
        "    args=training_args, # Now training_args is defined\n",
        "    train_dataset=train_dataset,\n",
        "    eval_dataset=test_dataset,\n",
        "    data_collator=lambda data: {\n",
        "        'input_ids': torch.stack([f['input_ids'] for f in data]),\n",
        "        'attention_mask': torch.stack([f['attention_mask'] for f in data]),\n",
        "        'labels': torch.stack([f['labels'] for f in data]),\n",
        "    }\n",
        ")\n",
        "# Create DataLoader for training and evaluation\n",
        "train_dataloader = DataLoader(train_dataset, batch_size=2, shuffle=True)\n",
        "eval_dataloader = DataLoader(test_dataset, batch_size=2)"
      ],
      "metadata": {
        "id": "ubpuyXGPF-Ir"
      },
      "execution_count": 6,
      "outputs": []
    },
