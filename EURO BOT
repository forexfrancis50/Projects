        "plt.title('EUR/USD Price Prediction')\n",
        "plt.xlabel('Time')\n",
        "plt.ylabel('Price')\n",
        "plt.legend()\n",
        "plt.show()\n",
        "\n",
        "model.save('forex_lstm_model.h5')\n",
        "\n"
